select candidato.nome 
     from (eleicao.votacao inner join eleicao.candidato 
     on votacao.idcandidato_candidato = candidato.idcandidato) ;

select candidato.nome, eleitor.nome
     from ((eleicao.votacao inner join eleicao.candidato
     on  votacao.idcandidato_candidato = candidato.idcandidato)
     inner join eleicao.eleitor on votacao.ideleitor_eleitor = eleitor.ideleitor);